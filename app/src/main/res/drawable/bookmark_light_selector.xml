<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_checked="true"
        android:drawable="@drawable/bookmark_dark_filled">
        <shape
            android:shape="rectangle">
            <size
                android:height="22dp"
                android:width="20dp"/>
        </shape>
    </item>
    <item
        android:state_checked="false"
        android:drawable="@drawable/bookmark_dark">
        <shape
            android:shape="rectangle">
            <size
                android:height="22dp"
                android:width="20dp"/>
        </shape>
    </item>
</selector>